{
    "dataRoles": [
        {
            "displayName": "ObjectId",
            "name": "dbId",
            "kind": "Grouping"
        },
        {
            "displayName": "Color",
            "name": "color",
            "kind": "Grouping"
        },
        {
            "displayName": "Opacity",
            "name": "opacity",
            "kind": "Grouping"
        },
        {
            "displayName": "Values",
            "name": "values",
            "kind": "Measure"
        }
    ],
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "dbId": {
                        "min": 1,
                        "max": 1
                    },
                    "color": {
                        "min": 0,
                        "max": 1
                    },
                    "opacity": {
                        "min": 0,
                        "max": 1
                    },
                    "values": {
                        "min": 0
                    }
                }
            ],
            "table": {
                "rows": {
                    "select": [
                        {
                            "for": {
                                "in": "dbId"
                            }
                        },
                        {
                            "for": {
                                "in": "color"
                            }
                        },
                        {
                            "for": {
                                "in": "opacity"
                            }
                        },
                        {
                            "for": {
                                "in": "values"
                            }
                        }
                    ],
                    "dataReductionAlgorithm": {
                        "window": {
                            "count": 30000
                        }
                    }
                }
            }
        }
    ],
    "objects": {
        "viewer": {
            "properties": {
                "accessTokenEndpoint": {
                    "type": {
                        "text": true
                    }
                }
            }
        },
        "design": {
            "properties": {
                "urn": {
                    "type": {
                        "text": true
                    }
                },
                "guid": {
                    "type": {
                        "text": true
                    }
                }
            }
        }
    },
    "privileges": [
        {
            "name": "WebAccess",
            "essential": true,
            "parameters": [
                "*"
            ]
        }
    ],
    "supportsMultiVisualSelection": true,
    "suppressDefaultTitle": true,
    "supportsLandingPage": true,
    "supportsEmptyDataView": true
}